{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "body": {
      "type": "richtext"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "editable",
        "pending",
        "rejected",
        "approved"
      ],
      "private": true,
      "default": "editable",
      "required": true
    },
    "reject_reason": {
      "type": "text"
    },
    "project_code": {
      "type": "string",
      "unique": true,
      "required": true
    }
  }
}
